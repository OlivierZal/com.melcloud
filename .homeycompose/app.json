  "api": {
    "getBuildings": {
      "method": "GET",
      "path": "/buildings"
    },
    "getFrostProtectionSettings": {
      "method": "GET",
      "path": "/buildings/:buildingId/settings/frost_protection"
    },
    "updateFrostProtectionSettings": {
      "method": "POST",
      "path": "/buildings/:buildingId/settings/frost_protection"
    },
    "getHolidayModeSettings": {
      "method": "GET",
      "path": "/buildings/:buildingId/settings/holiday_mode"
    },
    "updateHolidayModeSettings": {
      "method": "POST",
      "path": "/buildings/:buildingId/settings/holiday_mode"
    },
    "getLanguage": {
      "method": "GET",
      "path": "/language"
    },
    "getUnitErrorLog": {
      "method": "GET",
      "path": "/error_log"
    },
    "login": {
      "method": "POST",
      "path": "/login"
    },
    "getDeviceSettings": {
      "method": "GET",
      "path": "/devices/settings"
    },
    "setDeviceSettings": {
      "method": "POST",
      "path": "/devices/settings"
    },
    "getDriverSettings": {
      "method": "GET",
      "path": "/drivers/settings"
