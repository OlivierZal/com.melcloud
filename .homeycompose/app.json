  "api": {
    "getBuildings": {
      "method": "GET",
      "path": "/buildings"
    },
    "getErrors": {
      "method": "GET",
      "path": "/errors"
    },
    "getLanguage": {
      "method": "GET",
      "path": "/language"
    },
    "login": {
      "method": "POST",
      "path": "/sessions"
    },
    "getDeviceSettings": {
      "method": "GET",
      "path": "/settings/devices"
    },
    "setDeviceSettings": {
      "method": "PUT",
      "path": "/settings/devices"
    },
    "getDriverSettings": {
      "method": "GET",
      "path": "/settings/drivers"
    },
    "getFrostProtectionSettings": {
      "method": "GET",
      "path": "/settings/frost_protection/buildings/:buildingId"
    },
    "setFrostProtectionSettings": {
      "method": "PUT",
      "path": "/settings/frost_protection/buildings/:buildingId"
    },
    "getHolidayModeSettings": {
      "method": "GET",
      "path": "/settings/holiday_mode/buildings/:buildingId"
    },
    "setHolidayModeSettings": {
      "method": "PUT",
      "path": "/settings/holiday_mode/buildings/:buildingId"
    }
  },
